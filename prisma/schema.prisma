

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      String   @id @default(cuid())
  username   String       @unique
  email      String       @unique
  password   String
  cronJobs   CronJob[]
}

model CronJob {
  id      String   @id @default(cuid())
  name    String
  schedule String
  userId  String
  user    User     @relation(fields: [userId], references: [id])
}